{
  "name": "iquest",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.x",
    "pnpm": ">=8.x"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build ",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "typecheck": "vue-tsc -p tsconfig.app.json --noEmit",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@vuepic/vue-datepicker": "^8.1.1",
    "animate.css": "^4.1.1",
    "axios": "^1.6.7",
    "pinia": "^2.1.7",
    "swiper": "^11.0.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vuetify": "^3.5.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@mdi/font": "^7.4.47",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.71.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.5",
    "vite": "^5.1.4",
    "vite-plugin-vuetify": "^2.0.2",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "cd front && npx lint-staged && npm run typecheck",
    "post-commit": "git status"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
