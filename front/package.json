{
  "name": "iquest",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.x",
    "pnpm": ">=8.x"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build ",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "typecheck": "vue-tsc -p tsconfig.app.json --noEmit",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "pinia": "^2.1.7",
    "pnpm": "^8.15.3",
    "swiper": "^11.0.6",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.20",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "sass": "^1.71.1",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "cd front && npx lint-staged && npm run typecheck",
    "post-commit": "git status"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
